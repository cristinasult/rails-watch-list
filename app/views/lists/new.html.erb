<h2>Create a new list</h2>

<%= simple_form_for(@list) do |f| %>
  <div class="form-inputs">
    <%= f.input :photo, as: :file %>
    <%= f.input :name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<div>
  <%= link_to "Back to my list", lists_path %>
</div>
